    <!-- 深色丝袜内衣 -->
    <ThingDef ParentName="Wolfein_UnderwearBase_II">
        <defName>Wolfein_Black_Stocking</defName>
        <label>Wolfein Black_Stocking</label>
        <description>A comfortable dark-colored lingerie set paired with over-the-knee stockings.</description>
        <costList>
            <Cloth>50</Cloth>
        </costList>
        <graphicData>
            <texPath>Wolfein/Apparel/Underwear/Black_Stocking/Black_Stocking</texPath>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <statBases>
            <MaxHitPoints>150</MaxHitPoints>
            <WorkToMake>4000</WorkToMake>
            <Mass>0.2</Mass>
            <Flammability>0.5</Flammability>
            <ArmorRating_Sharp>0.1</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.05</ArmorRating_Blunt>
            <ArmorRating_Heat>0.1</ArmorRating_Heat>
            <Insulation_Cold>10</Insulation_Cold>
            <Insulation_Heat>5</Insulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <apparel>
            <useWornGraphicMask>false</useWornGraphicMask>
            <developmentalStageFilter>Child, Adult</developmentalStageFilter>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Legs</li>
                <li>Feet</li>
            </bodyPartGroups>
            <wornGraphicPath>Wolfein/Apparel/Underwear/Black_Stocking/Black_Stocking</wornGraphicPath>
            <tags>
                <li>Wolfein_Underwear_Daily</li>
                <li>Wolfein_Underwear_BlackWear</li>
            </tags>
            <defaultOutfitTags>
                <li>Worker</li>
                <li>Soldier</li>
            </defaultOutfitTags>
        </apparel>
        <equippedStatOffsets>
            <MentalBreakThreshold>-0.02</MentalBreakThreshold>
        </equippedStatOffsets>
        <colorGenerator Class="ColorGenerator_Options">
            <options>
                <li>
                    <weight>100</weight>
                    <only>(255,255,255)</only>
                </li>
            </options>
        </colorGenerator>
    </ThingDef>

